@using FS.Abp.AspNetCore.Mvc.UI.Theme.Unice.Themes.Unice.Components.Menu
@using FS.Abp.AspNetCore.Mvc.UI.Theme.Unice.Themes.Unice.Components.Toolbar

<nav>
    <a class="d-inline-block m-r-auto" href="@Url.Content("~/")">
        <img alt=""
             class="img-fluid" style="max-height:50px"
             src="@Url.Content("~/CFTA/LogoBrand.png")" />
    </a>
    <div class="responsive-btn">
        <a class="toggle-nav" href="#">
            <i aria-hidden="true" class="fa fa-bars"></i>
        </a>
    </div>
    <div class="navbar m-l-auto" id="togglebtn">
        <div class="responsive-btn">
            <h5 class="btn-back">back</h5>
        </div>
        <ul class="main-menu">

            @(await Component.InvokeAsync<MainNavbarMenuViewComponent>())
            @(await Component.InvokeAsync<MainNavbarToolbarViewComponent>())
        </ul>
    </div>
</nav>

